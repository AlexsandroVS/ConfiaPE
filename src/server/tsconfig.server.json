{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist-server",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist", "build"]
}
